    "url": "https://gitlab.com/snipet/sdk/nodejs-snipet/issues"
  },
  "homepage": "https://gitlab.com/snipet/sdk/nodejs-snipet#readme",
  "devDependencies": {
    "@kubb/cli": "^4.12.15",
    "@kubb/core": "^4.12.15",
    "@kubb/plugin-client": "^4.12.15",
    "@kubb/plugin-oas": "^4.12.15",
    "@kubb/plugin-ts": "^4.12.15",
    "@types/node": "^24.10.1",
    "dotenv": "^17.2.3",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
