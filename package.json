{
  "name": "@snipet/nodejs-client",
  "version": "0.1.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "npm run generate && tsup src/index.ts --format esm,cjs --dts",
    "generate": "kubb generate",
    "example": "node scripts/run-example.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/snipet/sdk/nodejs-snipet.git"
